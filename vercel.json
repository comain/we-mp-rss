{
  "version": 2,
  "builds": [
    {
      "src": "web_ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "vercel_entry.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel_entry.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "APP_NAME": "we-mp-rss",
    "ENABLE_JOB": "False"
  }
}